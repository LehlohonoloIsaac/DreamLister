# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'DreamLister' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DreamLister
  pod 'SwiftLint'

  target 'DreamListerTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SwiftLint'
  end

  target 'DreamListerUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SwiftLint'
  end

end
